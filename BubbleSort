
def bubbleSort(args):
    temp
    i = 0
    j= len(args) -1
    pivot = args(i+j)/2
    
    while args[i] > pivot:
        i+=
        
    while args[j] < pivot:
        j-=
        
    if args[i] > args[j]:
        swap (args[i],args[j])
        
  i+=
  j-=   
  
 def swap( x ,y):
    temp = x
    x = y
    y = temp
    
 def __init__ == __main__
    bubbleSort(1,6,2,5,8)
    print (bubbleSort)   
                   